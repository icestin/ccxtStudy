{
    "appenders": {
      "stdout": {
        "type": "stdout"
      },
      "file": {
        "type": "dateFile",
        "filename": "./logs/ticker.log",
        "layout": {
          "type": "dummy"
        },
        "pattern": ".yyyy-MM-dd-hh",
        "alwaysIncludePattern": true
      },
      "err": {
        "type": "dateFile",
        "filename": "./logs/err/err-ticker.log",
        "pattern": ".yyyy-MM-dd",
        "layout": {
          "type": "dummy"
        },
        "alwaysIncludePattern": true
      }
    },
    "categories": {
      "default": {
        "appenders": [
          "file"
        ],
        "level": "info"
      },
      "err": {
        "appenders": [
          "err"
        ],
        "level": "error"
      }
    }
  }